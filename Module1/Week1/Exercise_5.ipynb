    "import math\n",
    "#hàm căn bậc\n",
    "def cb(a,b):\n",
    "    c = a**(1/b)\n",
    "    return c\n",
    "    \n",
    "\n",
    "    \n",
    "def md_nre_single_sample():\n",
    "    y,yh,n,b = input().split()\n",
    "    y = float(y)\n",
    "    yh = float(yh)\n",
    "    n = int(n)\n",
    "    p = int(b)\n",
    "    result = ( cb(y,n) - cb(yh,n) ) **p\n",
    "    return result\n",
